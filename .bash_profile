export EDITOR=vim
export PATH=/usr/local/bin:$HOME/bin:/usr/local/mysql/bin:$PATH
export PYTHONPATH=/usr/local/lib/python2.7/site-packages/
export ARCHFLAGS="-arch x86_64"

# use the next line if you do not have oh-my-zsh installed
# source ~/.bash_prompt

source ~/.aliases
source ~/.functions
